            <wsdl:input>
                <soap:body use="literal" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" />
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="BS_GENERICPROCESS">
        <wsdl:port name="BS_GENERICPROCESSSoap" binding="s0:BS_GENERICPROCESSSoap">
            <soap:address
                location="http://localhost:7001/somepath" />
        </wsdl:port>
    </wsdl:service>
