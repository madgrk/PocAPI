            xmlns="http://www.accenture.com/assets/sdp/commonDataModel/asynch">
            <xs:element name="TSO_DATA">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element ref="TSOheader" />
                        <xs:element ref="TSOattributes" minOccurs="0" />
                        <xs:element ref="TSOresult" minOccurs="0" />
